/* pages/withdrawal/withdrawal.wxss */
.with-content {
  margin-top: 120rpx;
}
.with-content .with-item {
  background: #fff;
  padding: 24rpx 32rpx;
  margin-top: 16rpx;
}
.with-content .with-item > h2 {
  height: 60rpx;
  font-size: 32rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: #000000;
  line-height: 60rpx;
}
.with-content .with-item > text {
  display: block;
  height: 56rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: #5E5E5E;
  line-height: 56rpx;
}
.withdrawal-tab {
  position: fixed;
  width: 100%;
  top: 0;
}
